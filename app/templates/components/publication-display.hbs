{{log allFieldObjects}}
<table class="table table-striped table-bordered">
  <tbody>

    <tr>
      <th>Publikations-ID</th>
      <td>{{publication.pubid}}</td>
    </tr>
    <tr>
      <th>Publikationstyp</th>
      <td>{{publication.publication_type_label}}</td>
    </tr>

    <tr>
      <th>Innehållsmärkning</th>
      <td>{{publication.content_type_label}}</td>
    </tr>

    {{#if allFieldObjects.title}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.title.label fieldValue=publication.title}}
    {{/if}}

    {{#if allFieldObjects.alt_title}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.alt_title.label fieldValue=publication.alt_title}}
    {{/if}}

    {{#if allFieldObjects.abstract}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.abstract.label fieldValue=publication.abstract type='collapsed'}}
    {{/if}}

    {{#if allFieldObjects.pubyear}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.pubyear.label fieldValue=publication.pubyear}}
    {{/if}}

    {{#if allFieldObjects.publanguage}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.publanguage.label fieldValue=publication.publanguage_label}}
    {{/if}}

    {{#if allFieldObjects.category_hsv_local}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.category_hsv_local.label fieldValue=publication.category_objects listValue='name_path' type='list'}}
    {{/if}}

    {{#if allFieldObjects.url}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.url.label fieldValue=publication.url}}
    {{/if}}

    {{#if publication.publication_identifiers}}
      {{publication-field-bibl-review fieldLabel=(t 'publicationIdentifier.label') fieldValue=publication.publication_identifiers listLabel='identifier_label' listValue='identifier_value' type='list'}}
    {{/if}}

    {{#if allFieldObjects.links}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.links.label fieldValue=publication.links}}
    {{/if}}

    {{#if allFieldObjects.keywords}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.keywords.label fieldValue=publication.keywords}}
    {{/if}}

    {{#if allFieldObjects.project}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.project.label fieldValue=publication.project_objects listValue='title' type='list'}}
    {{/if}}

    {{#if allFieldObjects.pub_notes}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.pub_notes.label fieldValue=publication.pub_notes}}
    {{/if}}

    {{#if allFieldObjects.authors}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.authors.label fieldValue=publication.authors type='authors'}}
    {{/if}}

    {{#if allFieldObjects.journal_id}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.journal_id.label fieldValue=publication.journal_id}}
    {{/if}}

    {{#if allFieldObjects.sourcechooser}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.sourcechooser.label fieldValue=publication.sourcechooser}}
    {{/if}}

    {{#if allFieldObjects.sourcetitle}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.sourcetitle.label fieldValue=publication.sourcetitle}}
    {{/if}}

    {{#if allFieldObjects.sourcevolume}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.sourcevolume.label fieldValue=publication.sourcevolume}}
    {{/if}}

    {{#if allFieldObjects.sourceissue}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.sourceissue.label fieldValue=publication.sourceissue}}
    {{/if}}

    {{#if allFieldObjects.sourcepages}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.sourcepages.label fieldValue=publication.sourcepages}}
    {{/if}}

    {{#if allFieldObjects.issn}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.issn.label fieldValue=publication.issn}}
    {{/if}}

    {{#if allFieldObjects.eissn}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.eissn.label fieldValue=publication.eissn}}
    {{/if}}

    {{#if allFieldObjects.article_number}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.article_number.label fieldValue=publication.article_number}}
    {{/if}}

    {{#if allFieldObjects.extent}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.extent.label fieldValue=publication.extent}}
    {{/if}}

    {{#if allFieldObjects.publisher}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.publisher.label fieldValue=publication.publisher}}
    {{/if}}

    {{#if allFieldObjects.place}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.place.label fieldValue=publication.place}}
    {{/if}}

    {{#if allFieldObjects.series}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.series.label fieldValue=publication.series_objects listValue='title' type='list'}}
    {{/if}}

    {{#if allFieldObjects.isbn}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.isbn.label fieldValue=publication.isbn}}
    {{/if}}

    {{#if allFieldObjects.editor}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.editor.label fieldValue=publication.editor}}
    {{/if}}

    {{#if allFieldObjects.dissdate}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.dissdate.label fieldValue=publication.dissdate}}
    {{/if}}

    {{#if allFieldObjects.dissopponent}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.dissopponent.label fieldValue=publication.dissopponent}}
    {{/if}}

    {{#if allFieldObjects.patent_applicant}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.patent_applicant.label fieldValue=publication.patent_applicant}}
    {{/if}}

    {{#if allFieldObjects.patent_application_number}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.patent_application_number.label fieldValue=publication.patent_application_number}}
    {{/if}}

    {{#if allFieldObjects.patent_application_date}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.patent_application_date.label fieldValue=publication.patent_application_date}}
    {{/if}}

    {{#if allFieldObjects.patent_number}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.patent_number.label fieldValue=publication.patent_number}}
    {{/if}}

    {{#if allFieldObjects.patent_date}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.patent_date.label fieldValue=publication.patent_date}}
    {{/if}}

    {{#if allFieldObjects.artwork_type}}
      {{publication-field-bibl-review fieldLabel=allFieldObjects.artwork_type.label fieldValue=publication.artwork_type}}
    {{/if}}
  </tbody>
</table>
