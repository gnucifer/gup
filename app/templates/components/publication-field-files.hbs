<dt>{{fieldLabel}}</dt>
<dd>
	<ul class="list-inline gup-file-list">
	  {{#each fieldValue as |listItem|}}
	    <li>
	    <a target="_blank" href="{{fileBaseUrl}}/{{listItem.id}}?token={{token}}">{{listItem.name}}</a> <a href="javascript:void()" {{action 'removeFile' listItem.id}} ><i class="fa gup-error fa-times" aria-hidden="true"></i></a></li>
	  {{/each}}
	</ul>
</dd>
