<div class="selectDiv">
  {{view Ember.Select 
    content=pubyears  
    optionValuePath="content.id"
    optionLabelPath="content.pubyear"
    value=pubyear
  }}
  {{view Ember.Select 
      value=pubtype
      content=controllers.application.publicationTypes
      optionValuePath="content.code"
      optionLabelPath="content.name"
      prompt="VÃ¤lj"
  }}

</div>
<table class="table especial">
<tr class="reviewtable">
    <th id="pubidid"  >pubid</th>
    <th id="titleid"  >title</th>
    <th id="yearid"   >year</th>
    <th id="pubtypeid">pubtype</th>
</tr>
{{#each model as |publication|}}
<tr class="reviewtable">
    <td>{{publication.id}}</td>
    <td>{{#link-to 'publications.show' publication.pubid (query-params mode="bibreview")}}{{publication.title}}{{/link-to}}</td>
    <td>{{publication.pubyear}}</td>
    <td>{{publication.publication_type}}</td>
</tr>
{{/each}}
</table>

